{
  "name": "react-collapsed",
  "version": "0.1",
  "description": "Component to build flexible and accessible collapse panels",
  "main": "dist/collapse.min.js",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "build:watch": "rollup -w -c -o dist/collapse.min.js",
    "build": "rollup -c -o dist/collapse.min.js",
    "prepublish": "yarn build",
    "dev": "yarn concurrently \"yarn build:watch\" \"docz dev\"",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/roginfarrer/react-collapsed.git"
  },
  "keywords": [
    "collapse",
    "react",
    "collapsible",
    "animate",
    "height",
    "render"
  ],
  "author": "Rogin Farrer",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/roginfarrer/collapse/issues"
  },
  "homepage": "https://github.com/roginfarrer/collapse#readme",
  "peerDependencies": {
    "react": "15 || 16"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-syntax-object-rest-spread": "^7.0.0",
    "@babel/preset-flow": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "babel-plugin-react-flow-props-to-prop-types": "^0.15.0",
    "concurrently": "^4.0.1",
    "docz": "^0.12.5",
    "flow-bin": "^0.83.0",
    "jest": "^23.6.0",
    "jest-dom": "^2.1.0",
    "react": "^16.6.1",
    "react-dom": "^16.6.1",
    "react-testing-library": "^5.2.3",
    "regenerator-runtime": "^0.12.1",
    "rollup": "^0.67.1",
    "rollup-plugin-babel": "^4.0.3",
    "rollup-plugin-uglify": "^6.0.0"
  },
  "dependencies": {
    "raf": "^3.4.0"
  }
}

    "eslint-plugin-react": "^7.11.1",
    "flow-bin": "0.93.0",
    "jest": "^23.6.0",
    "jest-dom": "^2.1.0",
    "react": "^16.8.2",
    "react-dom": "^16.8.2",
    "react-testing-library": "^5.2.3",
    "regenerator-runtime": "^0.12.1",

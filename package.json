{
  "name": "react-collapsed",
  "version": "0.1.3",
  "description": "A small (< 3.5kb), headless component for creating flexible and accessible expand/collapse components.",
  "main": "dist/collapse.min.js",
  "files": [
    "dist/"
  ],
  "scripts": {
    "build:watch": "rollup -w -c -o dist/collapse.min.js",
    "build": "rollup -c -o dist/collapse.min.js",
    "prepublish": "yarn jest && yarn build",
    "dev": "yarn concurrently \"yarn build:watch\" \"docz dev\"",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/roginfarrer/react-collapsed.git"
  },
  "keywords": [
    "collapse",
    "react",
    "collapsible",
    "animate",
    "height",
    "render",
    "expand"
  ],
  "author": "Rogin Farrer",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/roginfarrer/react-collapsed/issues"
  },
  "homepage": "https://react-collapsed.netlify.com/",
  "peerDependencies": {
    "react": "15 || 16"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.5",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-syntax-object-rest-spread": "^7.0.0",
    "@babel/preset-env": "^7.1.5",
    "@babel/preset-flow": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "babel-core": "^7.0.0-0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "babel-plugin-react-flow-props-to-prop-types": "^0.15.0",
    "concurrently": "^4.0.1",
    "docz": "^0.12.5",
    "docz-plugin-css": "^0.11.0",
    "eslint": "^5.9.0",
    "eslint-plugin-flowtype": "^3.2.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.11.1",
    "flow-bin": "^0.83.0",
    "jest": "^23.6.0",
    "jest-dom": "^2.1.0",
    "react": "^16.6.1",
    "react-dom": "^16.6.1",
    "react-testing-library": "^5.2.3",
    "regenerator-runtime": "^0.12.1",
    "rollup": "^0.67.1",
    "rollup-plugin-babel": "^4.0.3",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-uglify": "^6.0.0"
  },
  "dependencies": {
    "raf": "^3.4.0"
  }
}
